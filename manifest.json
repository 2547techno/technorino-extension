{
    "manifest_version": 3,
    "name": "Technorino Extension",
    "description": "Extension for Chatterino fork, Technorino",
    "version": "1.1",
    "permissions": [
        "tabs",
        "nativeMessaging"
    ],
    "background": {
        "service_worker": "src/background.js"
    },
    "action": {
        "default_popup": "src/popup/index.html"
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
  }
